config DRIVER_TPM_LPC
	bool "LPC TPM driver"
	select DRIVER_TPM
	default n

config DRIVER_TPM_SLB9635_I2C
	bool "SLB9635 I2C TPM driver"
	select DRIVER_TPM_I2C
	select DRIVER_TPM
	default n

config DRIVER_TPM_I2C
	bool
	default n

config DRIVER_TPM
	bool
	default n
